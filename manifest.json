{
    "author": "Digit",
    "manifest_version": 2,
    "name": "SharpViewer",
    "version": "1.1.2",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "tabs",
        "<all_urls>"
    ],
    "icons": {
        "128": "icons/icon128.png",
        "16": "icons/icon16.png",
        "64": "icons/icon64.png",
        "48": "icons/icon48.png"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "sharp-viewer@rebitwise.com"
        }
    },
    "web_accessible_resources": [
        "sharp-viewer.html"
    ]
}